{"ast":null,"code":"import $ from 'jquery';\nimport * as bakeryName from '../../public/js/index/bakeryName.js';\nimport * as goldenCookie from '../../public/js/index/goldenCookie.js';\nimport { load, save } from '../../public/js/index/gameSaveManager.js';\nimport CookieButton from '../components/Cookie.vue';\nexport default {\n  name: 'CookieClicker',\n  components: {\n    CookieButton\n  },\n  methods: {\n    doSave: function () {\n      save();\n    },\n    doAcceptName: function () {\n      bakeryName.acceptName();\n    },\n    doRandomizeNameInput: function () {\n      bakeryName.randomizeNameInput();\n    },\n    doBringUpRenamePrompt: function () {\n      bakeryName.bringUpRenamePromt();\n    }\n  }\n};\n$(document).ready(function () {\n  bakeryName.populateJson();\n  goldenCookie.convertGCookieTimeToMS();\n  goldenCookie.setNextSpawnTime();\n  setInterval(goldenCookie.removeGoldenCookie, 100);\n  load();\n  window.setInterval(save, 60000);\n});","map":{"version":3,"mappings":"AAkFA,OAAOA,OAAO,QAAQ;AACtB,OAAO,KAAKC,UAAS,MAAO,qCAAqC;AACjE,OAAO,KAAKC,YAAW,MAAO,uCAAuC;AACrE,SAASC,IAAI,EAAEC,IAAG,QAAS,0CAA0C;AACrE,OAAOC,YAAW,MAAO,0BAAyB;AAElD,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,OAAO,EACP;IACIC,MAAM,EAAE,YAAU;MACdL,IAAI,EAAE;IACV,CAAC;IACDM,YAAY,EAAE,YAAU;MACpBT,UAAU,CAACU,UAAU,EAAE;IAC3B,CAAC;IACDC,oBAAoB,EAAE,YAAU;MAC5BX,UAAU,CAACY,kBAAkB,EAAE;IACnC,CAAC;IACDC,qBAAqB,EAAE,YAAU;MAC7Bb,UAAU,CAACc,kBAAkB,EAAE;IACnC;EACJ;AACJ;AAEAf,CAAC,CAACgB,QAAQ,CAAC,CAACC,KAAK,CAAC,YAAY;EAC1BhB,UAAU,CAACiB,YAAY,EAAE;EACzBhB,YAAY,CAACiB,sBAAsB,EAAE;EACrCjB,YAAY,CAACkB,gBAAgB,EAAE;EAC/BC,WAAW,CAACnB,YAAY,CAACoB,kBAAkB,EAAE,GAAG,CAAC;EACjDnB,IAAI,EAAE;EACNoB,MAAM,CAACF,WAAW,CAACjB,IAAI,EAAE,KAAK,CAAC;AACnC,CAAC,CAAC","names":["$","bakeryName","goldenCookie","load","save","CookieButton","name","components","methods","doSave","doAcceptName","acceptName","doRandomizeNameInput","randomizeNameInput","doBringUpRenamePrompt","bringUpRenamePromt","document","ready","populateJson","convertGCookieTimeToMS","setNextSpawnTime","setInterval","removeGoldenCookie","window"],"sourceRoot":"","sources":["C:\\Users\\lucas\\Downloads\\CookieClickerVue\\CookieClickerVue\\cookie-clicker_front-end\\src\\pages\\Index.vue"],"sourcesContent":["<template>\n    <div class=\"modal\" id=\"bakery-name-modal\">\n        <div class=\"modal-dialog modal-dialog-centered\">\n            <div class=\"modal-content dialog\">\n                <div class=\"modal-body\">\n                    <button type=\"button\" data-bs-target=\"#bakery-name-modal\"\n                        class=\"btn-close btn-close-white close-button\" aria-lavel=\"Close\"\n                        data-bs-dismiss=\"modal\"></button>\n                    <div class=\"header\">\n                        NAME YOUR BAKERY\n                    </div>\n                    <div class=\"text\">What should your bakery's name be?</div>\n                    <div class=\"input-container\">\n                        <input class=\"form-control\" id=\"name-input\" type=\"text\" maxlength=\"20\" />\n                    </div>\n                    <div class=\"button-box\">\n                        <button class=\"btn btn-outline-dark\" @click=\"doAcceptName()\"\n                            data-bs-dismiss=\"modal\">Confirm</button>\n                        <button class=\"btn btn-outline-dark\" @click=\"doRandomizeNameInput()\">Random</button>\n                        <button class=\"btn btn-outline-dark\" data-bs-dismiss=\"modal\">Cancel</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"notif-toasts toast-container bottom-0 start-50 translate-middle-x\" id=\"toast-container\">\n\n    </div>\n    <table id=\"game-window\">\n        <tr>\n            <td class=\"cookie column\">\n                <div id=\"bakery-name-banner\" @click=\"doBringUpRenamePrompt()\" data-bs-toggle=\"modal\" data-bs-target=\"#bakery-name-modal\"\n                    title=\"Rename Bakery\">\n                    <span id=\"bakery-name\">null</span>'s bakery\n                </div>\n\n                <div class=\"cookie-count-banner\">\n                    <div class=\"cookies\">\n                        <span id=\"cookie-counter\">0</span>\n                        <span> cookies</span>\n                    </div>\n                    <div class=\"cps\">\n                        <span>per second: </span>\n                        <span id=\"cps-counter\">0</span>\n                    </div>\n                </div>\n                <CookieClicker />\n            </td>\n            <td class=\"minigame column\">\n                <div class=\"news-section\">\n                    <table>\n                        <!--table w two rows and two columns-->\n                        <tr>\n                            <td class=\"button\">\n                                Options\n                            </td><!--this will span two rows-->\n                            <td class=\"news-column\" rowspan=\"2\">\n                            </td>\n                            <td class=\"button\">\n                                Info\n                            </td>\n                        </tr>\n                        <tr>\n                            <td class=\"button\">\n                                Stats\n                            </td>\n                            <td class=\"button\" @click=\"doSave()\">\n                                Save\n                            </td>\n                        </tr>\n                    </table>\n                </div>\n\n            </td>\n            <td class=\"upgrade column\">\n            </td>\n        </tr>\n    </table>\n</template>\n\n<script>\nimport $ from 'jquery';\nimport * as bakeryName from '../../public/js/index/bakeryName.js';\nimport * as goldenCookie from '../../public/js/index/goldenCookie.js';\nimport { load, save } from '../../public/js/index/gameSaveManager.js';\nimport CookieButton from '../components/Cookie.vue'\n\nexport default {\n    name: 'CookieClicker',\n    components: {\n        CookieButton\n    },\n    methods:\n    {     \n        doSave: function(){\n            save();\n        },\n        doAcceptName: function(){\n            bakeryName.acceptName();\n        },\n        doRandomizeNameInput: function(){\n            bakeryName.randomizeNameInput();\n        },\n        doBringUpRenamePrompt: function(){\n            bakeryName.bringUpRenamePromt();\n        }\n    }\n}\n\n$(document).ready(function () {\n    bakeryName.populateJson();\n    goldenCookie.convertGCookieTimeToMS();\n    goldenCookie.setNextSpawnTime();\n    setInterval(goldenCookie.removeGoldenCookie, 100);\n    load();\n    window.setInterval(save, 60000);\n});\n</script>\n\n<style>\n@import \"../../public/css/variables.css\";\n@import \"../../public/css/Index/dialog.css\";\n@import \"../../public/css/Index/index.css\";\n@import \"../../public/css/Index/news.css\";\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}