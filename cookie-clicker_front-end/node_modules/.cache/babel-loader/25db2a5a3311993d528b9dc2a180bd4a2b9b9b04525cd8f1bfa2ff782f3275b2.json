{"ast":null,"code":"import $ from 'jquery';\nimport * as bakeryName from '../../public/js/index/bakeryName.js';\nimport * as goldenCookie from '../../public/js/index/goldenCookie.js';\nimport * as cookieHandler from '../../public/js/index/cookieHandler.js';\nimport { load, save } from '../../public/js/index/gameSaveManager.js';\nexport default {\n  name: 'CookieClicker',\n  methods: {\n    doCookieClicked: function () {\n      console.log(\"clicked\");\n      cookieHandler.cookieClicked();\n    }\n  }\n};\n$(document).ready(function () {\n  bakeryName.populateJson();\n  goldenCookie.convertGCookieTimeToMS();\n  goldenCookie.setNextSpawnTime();\n  setInterval(goldenCookie.removeGoldenCookie, 100);\n  load();\n  window.setInterval(save, 60000);\n});","map":{"version":3,"mappings":"AAuFA,OAAOA,OAAO,QAAQ;AACtB,OAAO,KAAKC,UAAS,MAAO,qCAAqC;AACjE,OAAO,KAAKC,YAAW,MAAO,uCAAuC;AACrE,OAAO,KAAKC,aAAY,MAAO,wCAAwC;AACvE,SAASC,IAAI,EAAEC,IAAG,QAAS,0CAA0C;AAErE,eAAe;EACXC,IAAI,EAAE,eAAe;EACrBC,OAAO,EACP;IACIC,eAAe,EAAE,YACjB;MACIC,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;MACtBP,aAAa,CAACQ,aAAa,EAAE;IACjC;EACJ;AACJ;AAEAX,CAAC,CAACY,QAAQ,CAAC,CAACC,KAAK,CAAC,YAAY;EAC1BZ,UAAU,CAACa,YAAY,EAAE;EACzBZ,YAAY,CAACa,sBAAsB,EAAE;EACrCb,YAAY,CAACc,gBAAgB,EAAE;EAC/BC,WAAW,CAACf,YAAY,CAACgB,kBAAkB,EAAE,GAAG,CAAC;EACjDd,IAAI,EAAE;EACNe,MAAM,CAACF,WAAW,CAACZ,IAAI,EAAE,KAAK,CAAC;AACnC,CAAC,CAAC","names":["$","bakeryName","goldenCookie","cookieHandler","load","save","name","methods","doCookieClicked","console","log","cookieClicked","document","ready","populateJson","convertGCookieTimeToMS","setNextSpawnTime","setInterval","removeGoldenCookie","window"],"sourceRoot":"","sources":["C:\\Users\\lucas\\Downloads\\CookieClickerVue\\CookieClickerVue\\cookie-clicker_front-end\\src\\components\\Index.vue"],"sourcesContent":["<template>\n    <div class=\"modal\" id=\"bakery-name-modal\">\n        <div class=\"modal-dialog modal-dialog-centered\">\n            <div class=\"modal-content dialog\">\n                <div class=\"modal-body\">\n                    <button type=\"button\" data-bs-target=\"#bakery-name-modal\"\n                        class=\"btn-close btn-close-white close-button\" aria-lavel=\"Close\"\n                        data-bs-dismiss=\"modal\"></button>\n                    <div class=\"header\">\n                        NAME YOUR BAKERY\n                    </div>\n                    <div class=\"text\">What should your bakery's name be?</div>\n                    <div class=\"input-container\">\n                        <input class=\"form-control\" id=\"name-input\" type=\"text\" maxlength=\"20\" />\n                    </div>\n                    <div class=\"button-box\">\n                        <button class=\"btn btn-outline-dark\" onclick=\"acceptName()\"\n                            data-bs-dismiss=\"modal\">Confirm</button>\n                        <button class=\"btn btn-outline-dark\" onclick=\"randomizeNameInput()\">Random</button>\n                        <button class=\"btn btn-outline-dark\" data-bs-dismiss=\"modal\">Cancel</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"notif-toasts toast-container bottom-0 start-50 translate-middle-x\" id=\"toast-container\">\n\n    </div>\n    <table id=\"game-window\">\n        <tr>\n            <td class=\"cookie column\">\n                <div id=\"bakery-name-banner\" data-bs-toggle=\"modal\" data-bs-target=\"#bakery-name-modal\"\n                    title=\"Rename Bakery\">\n                    <span id=\"bakery-name\">null</span>'s bakery\n                </div>\n\n                <div class=\"cookie-count-banner\">\n                    <div class=\"cookies\">\n                        <span id=\"cookie-counter\">0</span>\n                        <span> cookies</span>\n                    </div>\n                    <div class=\"cps\">\n                        <span>per second: </span>\n                        <span id=\"cps-counter\">0</span>\n                    </div>\n                </div>\n                <div class=\"cookie-container\">\n                    <img class=\"shine\" draggable=\"false\" src=\"/images/shine.png\" />\n                    <!--two classes shine and reverse-->\n                    <img class=\"shine reverse\" draggable=\"false\" src=\"/images/shine.png\" />\n                    <img onclick=\"doCookieClicked()\" id=\"cookie-button\" src=\"/images/cookie.png\" draggable=\"false\" />\n                </div>\n            </td>\n            <td class=\"minigame column\">\n                <div class=\"news-section\">\n                    <table>\n                        <!--table w two rows and two columns-->\n                        <tr>\n                            <td class=\"button\">\n                                Options\n                            </td><!--this will span two rows-->\n                            <td class=\"news-column\" rowspan=\"2\">\n                            </td>\n                            <td class=\"button\">\n                                Info\n                            </td>\n                        </tr>\n                        <tr>\n                            <td class=\"button\">\n                                Stats\n                            </td>\n                            <td class=\"button\" onclick=\"save()\">\n                                Save\n                            </td>\n                        </tr>\n                    </table>\n                </div>\n\n            </td>\n            <td class=\"upgrade column\">\n            </td>\n        </tr>\n    </table>\n</template>\n\n<script>\nimport $ from 'jquery';\nimport * as bakeryName from '../../public/js/index/bakeryName.js';\nimport * as goldenCookie from '../../public/js/index/goldenCookie.js';\nimport * as cookieHandler from '../../public/js/index/cookieHandler.js';\nimport { load, save } from '../../public/js/index/gameSaveManager.js';\n\nexport default {\n    name: 'CookieClicker',\n    methods:\n    {\n        doCookieClicked: function() \n        {\n            console.log(\"clicked\");\n            cookieHandler.cookieClicked();\n        }\n    }\n}\n\n$(document).ready(function () {\n    bakeryName.populateJson();\n    goldenCookie.convertGCookieTimeToMS();\n    goldenCookie.setNextSpawnTime();\n    setInterval(goldenCookie.removeGoldenCookie, 100);\n    load();\n    window.setInterval(save, 60000);\n});\n</script>\n\n<style>\n@import \"../../public/css/variables.css\";\n@import \"../../public/css/Index/dialog.css\";\n@import \"../../public/css/Index/index.css\";\n@import \"../../public/css/Index/news.css\";\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}